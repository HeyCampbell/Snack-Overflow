<%= form_for(:user, url: user_update_path) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :pic_url%>
  <%= f.text_field :pic_url %>
  <%= f.label :website_url %>
  <%= f.text_field :website_url %>
  <%= f.label :bio %>
  <%= f.text_field :bio %>
   <%= f.submit "Sign Up" %>
<% end %>
